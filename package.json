{
  "name": "triplet",
  "version": "1.0.0",
  "description": "simple graph editor for interactive concept mapping",
  "main": "resources/main.js",
  "scripts": {
    "dev": "shadow-cljs watch main renderer",
    "repl-renderer": "shadow-cljs cljs-repl renderer",
    "repl-main": "shadow-cljs cljs-repl main",
    "build": "shadow-cljs compile main renderer",
    "release": "npx shadow-cljs release main renderer",
    "clean": "rm -rf resources/public/js/* && rm -rf target"
  },
  "keywords": [
    "shadow-cljs",
    "reagent",
    "electron"
  ],
  "author": "Peter Jacobs <peter@crespyl.net>",
  "license": "GPL-3.0",
  "dependencies": {
    "highlight.js": "11.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "electron": "^16.0.7",
    "electron-packager": "^15.4.0",
    "shadow-cljs": "^2.16.12",
    "source-map-support": "^0.5.21"
  }
}

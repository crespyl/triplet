{
  "name": "triplet",
  "version": "0.0.1",
  "description": "simple graph editor for interactive concept mapping",
  "main": "resources/main.js",
  "scripts": {
    "server": "npx shadow-cljs server",
    "watch": "npx shadow-cljs watch main renderer",
    "watch-css": "npx sass --watch src/styles:resources/public/css",
    "electron": "npx electron .",
    "repl-renderer": "npx shadow-cljs cljs-repl renderer",
    "repl-main": "npx shadow-cljs cljs-repl main",
    "compile": "npx shadow-cljs compile main renderer",
    "release": "npx shadow-cljs release main renderer",
    "clean": "rm -rf resources/public/js/* && rm -rf target"
  },
  "keywords": [
    "shadow-cljs",
    "re-frame",
    "reagent",
    "electron",
    "cytoscape"
  ],
  "author": "Peter Jacobs <peter@crespyl.net>",
  "license": "GPL-3.0",
  "dependencies": {
    "cytoscape": "^3.21.2",
    "cytoscape-automove": "^1.10.3",
    "cytoscape-cola": "^2.5.1",
    "cytoscape-cose-bilkent": "^4.1.0",
    "highlight.js": "^11.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "electron": "^18.3.0",
    "electron-packager": "^15.4.0",
    "sass": "^1.52.3",
    "shadow-cljs": "^2.16.12",
    "source-map-support": "^0.5.21"
  }
}
